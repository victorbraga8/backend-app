{
  "version": 2,
  "builds": [
      {
          "src": "build/server.js",
          "use": "@vercel/node"          
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "build/server.js",
          "headers": {
            "Cache-Control": "s-maxage=1, stale-while-revalidate"
          },
          "continue": true
      }
  ]
}